<div id="player" data-playlistid={{playListId}}>
    <iframe id="ytplayer" type="text/html" width="400" height="400"
            src="https://www.youtube.com/embed/videoseries?list={{playListId}}&autoplay=1&controls=0&disablekb=1&enablejsapi=1&fs=0&loop=1&modestbranding=1&rel=0&showinfo=0&iv_load_policy=3"
            frameborder="0" allowfullscreen>
</div>
<div id="player-content">
    <div class="action">
        <div class="spacer" id="title">{{title}}</div>
            <script>"PLAYER GOES HERE"</script>
            <div class="buttonZone">
                <button type="button" id="backButt"><i class="button"></i></button>
                <button type="button" id="pausePlay"><i class="button"></i></button>
                <button type="button" id="skipButt"><i class="button"></i></button>
            </div>
        <div class="spacer"></div>
    </div>
</div>